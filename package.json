{
  "name": "my-app",
  "version": "0.0.1",
  "private": true,
  "externals": {
    "react": "React",
    "react-dom": "ReactDOM"
  },
  "scripts": {
    "build-tsx-browser": "parcel build --log-level 1 --global __root_component --out-dir dist/views views/*.tsx",
    "build-tsx-node": "parcel build --log-level 1 --no-minify --target node --out-dir dist/views/node views/*.tsx",
    "build-ts": "tsc",
    "build": "concurrently -i 'npm:build-*'",
    "postbuild": "cp -r views/templates dist/views/",
    "start": "node -r esm dist/app.js",
    "watch-server": "nodemon -e js -r esm dist/app.js",
    "watch-tsx-browser": "parcel watch --log-level 1 --no-hmr --global __root_component --out-dir dist/views views/*.tsx",
    "watch-tsx-node": "parcel watch --log-level 1 --target node --out-dir dist/views/node views/*.tsx",
    "watch-tsx": "concurrently 'npm:watch-tsx-browser' 'npm:watch-tsx-node'",
    "watch": "concurrently -i 'npm:watch-server' 'npm:watch-tsx-browser' 'npm:watch-tsx-node'",
    "lint": "eslint app.ts routes"
  },
  "dependencies": {
    "@atlaskit/button": "^15.1.0",
    "@atlaskit/checkbox": "^12.4.4",
    "@atlaskit/empty-state": "^7.4.6",
    "@atlaskit/form": "^8.8.3",
    "@atlaskit/progress-bar": "^0.5.15",
    "@atlaskit/section-message": "^5.0.7",
    "@atlaskit/textarea": "^4.5.4",
    "@atlaskit/textfield": "^5.3.4",
    "@atlaskit/tooltip": "^17.6.5",
    "@types/express": "^4.17.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "atlassian-connect-express": "^7.7.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "errorhandler": "^1.5.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-handlebars": "^6.0.6",
    "express-hbs": "^2.4.0",
    "helmet": "^4.1.1",
    "morgan": "^1.10.0",
    "nocache": "^2.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sequelize": "^6.19.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/errorhandler": "^1.5.0",
    "@types/morgan": "^1.9.4",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-styled-components": "^1.11.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.10.0",
    "eslint-plugin-react": "^7.21.3",
    "longjohn": "^0.2.12",
    "ngrok": "^3.3.0",
    "nodemon": "^2.0.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "^0.5.1",
    "sqlite3": "^5.0.0",
    "typescript": "^4.9.4"
  }
}
